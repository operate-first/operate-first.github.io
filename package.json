    "gatsby-plugin-manifest": "4.20.0",
    "gatsby-plugin-mantine": "4.0.0",
    "gatsby-plugin-react-helmet": "5.20.0",
    "gatsby-remark-prismjs": "6.20.0",
    "prism-react-renderer": "1.3.5",
